{
  "compilerOptions": {
    "lib": [
      "es2022"
    ],
    "types": [
      "bun-types"
    ],
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": false,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "strict": true,
    "verbatimModuleSyntax": false
  },
  "exclude": [
    "node_modules"
  ]
}
